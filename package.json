{
  "name": "markd",
  "version": "1.0.0",
  "description": "Bookmarks !",
  "authors": [
    "Gabe Dunn <gabe.dunn@shaw.ca>"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn lint",
    "build": "node build/build.js",
    "dev": "node build/dev-server.js",
    "serve": "node bin/markd",
    "serve:dev": "nodemon bin/markd --ignore src/ --ignore build/",
    "start": "npm-run-all --parallel build serve",
    "lint": "eslint --ext .js,.vue src",
    "mkenv": "envup",
    "preversion": "git pull && npm up && yarn lint",
    "version": "yarn build && git add -A .",
    "postversion": "git push --tags origin HEAD",
    "test": "ava | tnyan",
    "test:ci": "ava",
    "test:watch": "ava --watch"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.0.1",
    "dexie": "^1.5.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.3",
    "request": "^2.81.0",
    "reset-css": "^2.2.1",
    "serve-favicon": "^2.4.3",
    "vue": "^2.4.2",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^0.10.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.1.18",
    "browser-env": "^3.2.0",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "envup": "^1.0.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^3.13.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.1",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "ora": "^1.3.0",
    "postcss": "^6.0.10",
    "postcss-cssnext": "^3.0.2",
    "require-extension-hooks": "^0.3.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "semver": "^5.4.1",
    "shelljs": "^0.7.8",
    "tap-nyan": "^1.1.0",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "src/**/*.js",
      "src/**/*.vue"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": true,
    "require": [
      "babel-register",
      "./tests/helpers/setup.js"
    ],
    "babel": "inherit"
  }
}
